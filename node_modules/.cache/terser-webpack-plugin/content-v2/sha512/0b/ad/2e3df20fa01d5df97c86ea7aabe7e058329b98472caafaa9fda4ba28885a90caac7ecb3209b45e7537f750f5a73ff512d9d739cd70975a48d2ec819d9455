zes() }} />)\r\n\t}\r\n\r\n\r\n\r\n\treturn (\r\n\t\t<div className=\"App\">\r\n\t\t\t{g_ini}\r\n\t\t</div>\r\n\t);\r\n}\r\n\r\nexport default App;\r\n","import React from 'react';\r\nimport ReactDOM from 'react-dom';\r\nimport App from './App';\r\n\r\n\r\nReactDOM.render(\r\n  <React.StrictMode>\r\n    <App />\r\n  </React.StrictMode>,\r\n  document.getElementById('root')\r\n);\r\n\r\n"]}}